<%= form_tag [ :session ] do %>
  <div class="form-horizontal">
    <div class="form-group">
      <%= label_tag :identify_name, 'ログイン名', class: "col-xs-4" %>
      <div class="col-xs-8">
        <%= text_field_tag :identify_name, '', autofocus: true, class: "form-control" %>
      </div>
    </div>
    <div class="form-group">
      <%= label_tag :password, 'パスワード', class: "col-xs-4" %>
      <div class="col-xs-8">
        <%= password_field_tag :password, '', class: "form-control" %>
      </div>
    </div>
    <div class="form-group">
      <div class="col-xs-offset-2 col-xs-10">
        <%= submit_tag 'ログイン', class: 'btn btn-default' %>
      </div>
    </div>
    <div class="form-group">
      <div class="col-xs-offset-2 col-xs-10">
        <a href="", class="githubLink" id="githubButton",
          onclick="window.open('/auth/github', 'git', 'width=700, height=600');
          return false">Githubログイン</a>
      </div>
    </div>
  </div>
<% end %>

<script type="text/javascript">
  $.testFunc = function(val){
    location = (val);
  };
</script>
